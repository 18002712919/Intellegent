<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- 条形设置条属性 -->
    <declare-styleable name="LineSettingView">
        <attr name="subject" format="string" /> <!-- 主题 -->
        <attr name="title" format="string" />   <!-- 标题 -->
        <attr name="titles" format="string" />  <!-- 副标题 -->
        <attr name="content" format="string" /> <!-- 内容 -->
        <attr name="isLayout" format="boolean" /> <!-- 是否用此布局 -->
        <attr name="is_title" format="boolean" /> <!-- 是否有副标题 -->
        <attr name="canNav" format="boolean" /> <!-- 是否有开关 -->
        <attr name="isLinear" format="boolean" /> <!-- 是否有内容 -->
        <attr name="isBottom" format="boolean" /><!-- 是否是列表中最后一个 -->
        <attr name="layout_height" format="integer" /><!-- 控件height -->
    </declare-styleable>

</resources>